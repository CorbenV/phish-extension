{
	"name": "Phish Extension",
	"description": "Test For CODERS",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "src/popup/popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"js": ["scripts/content.js"],
			"matches": ["https://outlook.office.com/mail/*"]
		}
	],
	"permissions": ["scripting", "activeTab"]
}
